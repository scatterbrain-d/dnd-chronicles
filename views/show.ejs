<% include ./partials/header %>



    <div class="container">
        <h2><%= session.title %></h2>
        <p>Party Members:
            <% var party = session.characters; %>
            <% party.forEach(function(character){ %>    
                <a href="/characters/<%= character._id %>" ><%= character.name + " " %></a>
            <% }) %>
        </p>
        <p><%- session.content %></p>
        <a href="/<%= session._id %>/edit" type="button">Edit</a>
    </div>
<% console.log(session); %>

<% include ./partials/footer %>